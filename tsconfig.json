{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "resolvePackageJsonExports": false,
    "target": "esnext",
    "baseUrl": "./src",
    "paths": {
      "~/*": [
        "*"
      ]
    },
    "types": [
      "react-native"
    ],
    "lib": [
      "es2023",
      "dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}